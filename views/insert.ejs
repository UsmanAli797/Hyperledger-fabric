<h4 class="head-4">
    New Complaint info
</h4>
<%if(errors.length > 0) {%>

    <% for(var i=0;i< errors.length; i++ ){%>
        <div class="alert alert-danger">
            <%=errors[i]%>
        </div>
    <%}%>
<%}%>
<%if(success.length>0){%>
    <div class="alert alert-success"><%%>
        <%=success%>
    </div>
<%}%>
<form action="/register_c" method="post" class="needs-validation col-6"
style="padding-left: 70px;font-size: 20px;" novalidate>
<div class="form-group">
    <label>Location</label>
    <input type="text" class="form-control col-6" name="loc" required placeholder="Enter current Location">
    <div class="invalid-feedback">
        Please Enter your location
    </div>
</div>
<div class="form-group">
    <label>Complaint</label>
    <input type="text" class="form-control col-6" name="complaint" required placeholder="Enter complaint">
    <div class="invalid-feedback">
        Please enter your complaint
    </div>
</div>
<div class="form-group">
    <label>Evidence</label>
    <input type="text" class="form-control col-6" name="evidence" required placeholder="Provide Evidence">
    <div class="invalid-feedback">
        Please provide Evidence
    </div>
    <div class="form-group">
        <label>Name</label>
        <input type="text" class="form-control col-6" name="naa" required placeholder="Enter your name">
        <div class="invalid-feedback">
            Please provide your correct name
        </div>
        <div class="form-group">
            <label>Sex</label>
            <input type="text" class="form-control col-6" name="sex" required placeholder="Provide Sex">
            <div class="invalid-feedback">
                Please provide your sex
            </div>
</div>
<div class="form-group">
    <input type="submit" class="btn btn-primary col3" value="Submit">
</div>
</form>